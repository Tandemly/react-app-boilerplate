{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  // "extends": "eslint:recommended",
  "plugins": ["react", "prettier", "prettier/flowtype", "prettier/react"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
    // "no-console": "off",
    // "strict": ["error", "global"],
    // "curly": "warn"
  }
}
